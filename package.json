{
  "name": "Application",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": "^6.0.0 || ^7.0.0",
    "npm": "^3.0.0"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/src/common/create-store.js"
    ]
  },
  "scripts": {
    "start": "npm run build:prod",
    "clean": "[ -d dist ] && trash dist || true",
    "coverage": "npm run test -- --coverage",
    "coverage:watch": "npm run coverage -- --watch",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "server": "npm run server:hot",
    "server:hot": "cross-env NODE_ENV=development HOT_MODULES=true node hot-server",
    "server:story": "cross-env NODE_ENV=storybook start-storybook -p 9001",
    "build": "npm run clean && cross-env HOT_MODULES=false webpack",
    "build:docs": "esdoc -c esdoc.json",
    "build:dev": "cross-env NODE_ENV=development npm run build",
    "build:prod": "cross-env NODE_ENV=production npm run build",
    "build:story": "cross-env NODE_ENV=storybook build-storybook -o story"
  },
  "dependencies": {
    "babel-core": "^6.18.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-transform-inline-environment-variables": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-3": "^6.17.0",
    "copy-webpack-plugin": "^4.0.0",
    "cross-env": "^3.1.1",
    "css-loader": "^0.26.1",
    "file-loader": "^0.9.0",
    "globule": "^1.0.0",
    "json-loader": "^0.5.4",
    "normalize.css": "^5.0.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-redux": "^5.0.1",
    "redux": "^3.6.0",
    "redux-actions": "^1.2.0",
    "redux-saga": "^0.13.0",
    "style-loader": "^0.13.1",
    "trash-cli": "^1.3.0",
    "webpack": "2.2.0-rc.0"
  },
  "devDependencies": {
    "@kadira/storybook": "^2.34.0",
    "babel-eslint": "^7.0.0",
    "babel-preset-stage-0": "^6.16.0",
    "enzyme": "^2.6.0",
    "esdoc": "^0.4.8",
    "esdoc-es7-plugin": "^0.0.3",
    "eslint": "^3.8.1",
    "eslint-config-jupl": "^0.5.0",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-react": "^6.6.0",
    "eventsource-polyfill": "^0.9.6",
    "hapi": "^16.0.1",
    "hapi-webpack-plugin": "^1.3.0",
    "jest": "^18.0.0",
    "react-addons-test-utils": "^15.3.2",
    "react-test-renderer": "^15.3.2",
    "redux-devtools-extension": "^1.0.0",
    "redux-logger": "^2.7.4"
  }
}
