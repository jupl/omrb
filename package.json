{
  "private": true,
  "engines": {
    "node": "^5.0.0",
    "npm": "^3.0.0"
  },
  "scripts": {
    "clean": "[ -d dist ] && trash dist || true",
    "lint": "eslint . --ext=js --ext=jsx",
    "server:hot": "better-npm-run server:hot",
    "build:dev": "npm run clean && better-npm-run build:dev",
    "build:prod": "npm run clean && better-npm-run build:prod"
  },
  "betterScripts": {
    "server:hot": {
      "command": "node hot-server",
      "env": {
        "NODE_ENV": "development",
        "BABEL_ENV": "hot"
      }
    },
    "build:dev": {
      "command": "webpack",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build:prod": {
      "command": "webpack",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "dependencies": {
    "normalize.css": "^4.1.1",
    "react": "^15.0.1",
    "react-dom": "^15.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.7.6",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-inline-environment-variables": "^6.5.0",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-2": "^6.5.0",
    "better-npm-run": "0.0.8",
    "copy-webpack-plugin": "^1.1.1",
    "css-loader": "^0.23.1",
    "eslint": "^2.7.0",
    "eslint-config-jupl": "^0.3.2",
    "eslint-plugin-babel": "^3.2.0",
    "eslint-plugin-react": "^4.3.0",
    "eventsource-polyfill": "^0.9.6",
    "file-loader": "^0.8.5",
    "globule": "^0.2.0",
    "hapi": "^13.3.0",
    "hapi-webpack-plugin": "^1.3.0",
    "style-loader": "^0.13.1",
    "trash": "^3.4.1",
    "webpack": "^1.12.15"
  }
}
